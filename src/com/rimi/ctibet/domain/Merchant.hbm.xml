<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.rimi.ctibet.domain.Merchant" table="merchant">
		<id name="id" type="java.lang.Integer">
			<column name="id" length="40" />
			<generator class="native" />
		</id>
		<property name="code">
			<column name="code" />
		</property>
		<property name="merchantName">
			<column name="merchantName" />
		</property>
		<property name="viewCode">
			<column name="viewcode" />
		</property>
		<property name="distination">
			<column name="distination" />
		</property>
		<property name="merchantType">
			<column name="merchantType" />
		</property>
		<property name="isRecommend">
			<column name="isRecommend" />
		</property>
		<property name="merchantImage">
			<column name="merchantImage" />
		</property>
		<property name="merchantSummary">
			<column name="merchantSummary" />
		</property>
		<property name="merchantDetail">
			<column name="merchantDetail"/>
		</property>
		<property name="createTime">
			<column name="createTime" />
		</property>
		<property name="lastEditTime">
			<column name="lastEditTime" />
		</property>
		<property name="avaliable">
			<column name="avaliable" />
		</property>
		<property name="ctripUrl">
			<column name="ctripUrl" />
		</property>
		<property name="url">
			<column name="url" />
		</property>
		<property name="keyWord">
			<column name="keyWord" />
		</property>
		<property name="price">
			<column name="price" />
		</property>
		<property name="activityUrl">
			<column name="activityUrl" />
		</property>
        <property name="tdkTitle"       column="tdkTitle"/>
      	<property name="tdkDescription" column="tdkDescription"/>
        <property name="tdkKeywords"    column="tdkKeywords"/>
	</class>
</hibernate-mapping>
