<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.rimi.ctibet.domain.CommonOrder" table="COMMON_ORDER">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="code" column="code" type="java.lang.String"/>

		<property name="orderCode"       column="orderCode"       type="java.lang.String"/>
		<property name="orderName"       column="orderName"       type="java.lang.String"/>
		<property name="note"            column="note"            type="java.lang.String"/>
		<property name="payState"        column="payState"/>
		<property name="money"           column="money"/>
		<property name="alipayOrderCode" column="alipayOrderCode" type="java.lang.String"/>
		<property name="dealTime"        column="dealTime"        type="java.util.Date"/>
		<property name="alipayEmail"     column="alipayEmail"     type="java.lang.String"/>
		<property name="type"            column="type"            type="java.lang.String"/>
		<property name="source"          column="source"          type="java.lang.String"/>
		<property name="successUrl"      column="successUrl"      type="java.lang.String"/>
		<property name="failureUrl"      column="failureUrl"      type="java.lang.String"/>
		
		<property name="num"        column="num"/>
		<property name="goodsCode"  column="goodsCode"  type="java.lang.String"/>
		<property name="receiver"   column="receiver"   type="java.lang.String"/>
		<property name="mobile"     column="mobile"     type="java.lang.String"/>
		<property name="address"    column="address"    type="java.lang.String"/>
		<property name="zipcode"    column="zipcode"    type="java.lang.String"/>
		<property name="memberCode" column="memberCode" type="java.lang.String"/>
        
        <property name="createTime" column="createTime" type="java.util.Date" update="false"/>
        <property name="lastEditTime" column="lastEditTime" type="java.util.Date"/>
        <property name="avaliable" column="avaliable" update="false"/>
	</class>
</hibernate-mapping>
