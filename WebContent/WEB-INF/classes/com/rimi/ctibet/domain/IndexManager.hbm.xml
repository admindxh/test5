<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.rimi.ctibet.domain.IndexManager" table="indexmanager">
        <id name="id" type="java.lang.Integer">
            <column name="id" length="40" />
            <generator class="native" />
        </id>
        <property name="code">
            <column name="code"/>
        </property>
        
        <property name="pramaCode" column="programacode" type="java.lang.String">
        </property>
        <property name="url" column="url" type="java.lang.String">
        </property>
        <property name="number" column="number" type="java.lang.String">
        </property>
        
        <property name="replyname" column="replyname" type="java.lang.String">
        </property>
        <property name="praise" column="praise" >
        </property>
        <property name="replycontent" column="replycontent" type="java.lang.String">
        </property>
        
        
        <property name="createTime">
            <column name="createTime"/>
        </property>
        <property name="lastEditTime">
            <column name="lastEditTime"/>
        </property>
        <property name="avaliable">
            <column name="available"/>
        </property>
       
    </class>
</hibernate-mapping>
