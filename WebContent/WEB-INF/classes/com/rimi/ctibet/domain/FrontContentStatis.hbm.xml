<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.rimi.ctibet.domain.FrontContentStatis" table="FronotContentStatis">
        <id name="id" type="java.lang.Integer">
            <column name="id" length="40" />
            <generator class="native" />
        </id>
        <property name="progtype">
            <column name="progtype"/>
        </property>
        
        <property name="actiontype">
            <column name="actiontype"/>
        </property>
	
	<property name="tablename">
            <column name="tablename"/>
        </property>        
        
        
        <property name="code">
            <column name="code"/>
        </property>  
		
		<property name="ctime" column="ctime" type="java.util.Date"></property>        
        
    </class>
</hibernate-mapping>
