<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.rimi.ctibet.domain.Programa" table="programa">
        <id name="id" type="java.lang.Integer">
            <column name="id" length="40" />
            <generator class="native" />
        </id>
        <property name="code">
            <column name="code"/>
        </property>
        <property name="programaName">
            <column name="programaName"/>
        </property>
        <property name="enName">
            <column name="enName"/>
        </property>
        <property name="programaSummary">
            <column name="programaSummary" />
        </property>
        <property name="pCode">
            <column name="pCode"/>
        </property>
        <property name="programaUrl">
            <column name="programaUrl" />
        </property>
        <property name="imageUrl">
            <column name="imageUrl"/>
        </property>
        <property name="rank">
            <column name="rank" />
        </property>
         <property name="remark">
            <column name="remark" />
        </property>
         <property name="createUserCode">
            <column name="createUserCode" />
        </property>
        <property name="createTime">
            <column name="createTime"/>
        </property>
        <property name="lastEditTime">
            <column name="lastEditTime"/>
        </property>
        <property name="available">
            <column name="available"/>
        </property>
        <property name="typeCode">
            <column name="typeCode"/>
        </property>
    </class>
</hibernate-mapping>
