<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.rimi.ctibet.domain.RideRecommon" table="riderecomm">
        <id name="id" type="java.lang.Integer">
            <column name="id" length="40" />
            <generator class="native" />
        </id>
        <property name="code">
            <column name="code"/>
        </property>
        
        <property name="avaliable" column="avaliable" type="java.lang.Integer">
        </property>
        
        <property name="ctorname" column="ctorname" type="java.lang.String"></property>
        <property name="contentType" column="contentType" type="java.lang.String"></property>
        <property name="imgurl" column="imgurl" type="java.lang.String"></property>
        <property name="url" column="url" type="java.lang.String"></property>
       
        
        
        
        <property name="createTime">
            <column name="createTime"/>
        </property>
       
       
    </class>
</hibernate-mapping>
