<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.rimi.ctibet.domain.UserSysMessage" table="user_sysmessage">
        <id name="id" type="java.lang.Integer">
            <column name="id" length="40" />
            <generator class="native" />
        </id>
         <property name="memberCode" column="memberCode" type="java.lang.String" />
         <property name="msgCode" column="msgCode" type="java.lang.String" />
         <property name="type" column="type" type="java.lang.String" />
         <property name="state" column="state" type="java.lang.String" />
    </class>
</hibernate-mapping>
